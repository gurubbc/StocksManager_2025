-- customers table
CREATE TABLE CUSTOMERSSS(Cust_id NUMBER(3) PRIMARY KEY, 
  FIRST_NAME VARCHAR2(20), 
  LAST_NAME VARCHAR2(20), 
  PHONE_NUMBER NUMBER(10), 
  CITY VARCHAR2(20),
  EMAIL_ID VARCHAR2(15));

