--transaction table
CREATE TABLE TRANSACTIONS(
TXN_ID NUMBER(3) PRIMARY KEY, 
CUST_ID NUMBER(3),
stock_id NUMBER(3),
txn_price NUMBER(8,2),
txn_type VARCHAR2(10),
qty NUMBER(10),
txn_date DATE);

